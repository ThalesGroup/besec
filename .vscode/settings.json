{
    "json.schemas": [
        {
            "schema.json": "http://json-schema.org/draft-07/schema"
        }
    ],
    "yaml.schemas": {

        "./practices/schema.json": ["/practices/*.yaml", "examplePractice.yaml"],
        "http://swagger.io/v2/schema.json": "api/swagger.yaml",
        "https://json.schemastore.org/github-workflow.json": ".github/workflows/*.yml"
    },
    "yaml.format.printWidth": 120,
    "yaml.format.enable": true,
    "gopls.env": {
        "GOFLAGS": "-tags=assets"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "typescriptreact",
            "autoFix": true
        }
    ]
}
